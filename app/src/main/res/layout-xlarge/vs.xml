<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/button3"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">
    <!--tools:context=".MyPageServlet" >-->

    <Button
        android:id="@+id/button3"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignParentLeft="true"
        android1:background="@drawable/my"
        android1:layout_alignParentTop="true"
        android1:textColor="#A9A9A9"
        android1:text="Myぺージ" />

    <ImageView
        android1:id="@+id/imageButton1"
        android1:layout_width="90dp"
        android1:layout_height="80dp"
        android1:layout_alignLeft="@+id/button3"
        android1:layout_below="@+id/button3"
        android1:layout_marginTop="15dp"
        android1:src="@drawable/design_img_f_1431986_s" />

    <Button
        android1:id="@+id/button5"
        android1:layout_width="60dp"
        android1:layout_height="30dp"
        android1:layout_alignBottom="@+id/imageButton1"
        android1:layout_alignParentRight="true"
        android1:padding="1dp"
        android1:text="棋譜"
        android1:textSize="13sp" />

    <TextView
        android1:id="@+id/textView3"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/textView1"
        android1:layout_alignBottom="@+id/textView1"
        android1:layout_alignLeft="@+id/button5"
        android1:text="	勝敗"
        android1:textSize="16sp" />

    <TextView
        android1:id="@+id/textView1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/textView2"
        android1:layout_alignBottom="@+id/textView2"
        android1:layout_marginRight="42dp"
        android1:layout_toLeftOf="@+id/textView3"
        android1:text="ID"
        android1:textSize="16sp" />

    <TextView
        android1:id="@+id/textView2"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignTop="@+id/imageButton1"
        android1:layout_toRightOf="@+id/imageButton1"
        android1:text="称号"
        android1:textSize="16sp" />

</RelativeLayout>